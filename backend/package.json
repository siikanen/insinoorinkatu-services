{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/server",
    "dev": "node --trace-warnings ./node_modules/nodemon/bin/nodemon.js ./bin/server",
    "test": "mocha",
    "migration": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "*.test.js",
      ".git",
      "node_modules/**/node_modules",
      "*-journal",
      "*.sqlite"
    ],
    "NODE_ENV": "development",
    "ext": "js,yaml"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "faker": "^5.1.0",
    "mocha": "^8.2.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "nodemon": "^2.0.6",
    "sqlite3": "^5.0.0",
    "supertest": "^6.0.1",
    "swagger-ui": "^3.20.7",
    "uuid": "^8.3.2"
  }
}
